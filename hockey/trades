  <script>
   var imgdir="https://capfriendly-wlb8ng5.stackpathdns.com/assets/images/";
var imgdir_user="https://capfriendly-avatars-wlb8ng5.stackpathdns.com/";
var cookie_domain="capfriendly.com";
var site_title_detail = "NHL Salary Caps";
var current_season = 2021;

$(document).ready(function(){
	w = $(window).width()-20;
	h = $(window).height();
	if(h<600){
		var h1 = '400';
	}else{
		var h1 = 'auto';
	}
	if(parseInt($("#infraction_count").attr("data-value")) > 0){
		$("#infraction_notf").dialog({
			title: "Warning/Infraction Notice",
			autoOpen:false,
			resizable: false,
			width: w,
			modal: true,
			height: h1,
			hide: {
				effect: 'fade',
				duration: 500
			},
		 });
		$("#infraction_notf").dialog('open');
		$('.ui-dialog-titlebar-close').hide(); //Hide the corner close button
	 }
});
$('body').on('click','#submitAcknowledgment',function(e){
	$.post('/ajax/settings/infraction/acknowledge',{
		token:token
	}, function(d) {
		if (d.meta.code == 200) {
			$("#infraction_notf").dialog('close');
		}
	},"JSON");
});

function displayAdBlockerMessage(){
	var ablk_alrt = "<b>AdBlock Detected</b><div class='p3'>Please consider whitelisting CapFriendly.com from your ad blocker. Ads help us host the site.</div>If you find any ads intrusive or inappropriate, please <a href='/contact'>contact us</a>.";
	$('.ablk_alrt').addClass("ablk_dsply").html(ablk_alrt).show();
}
if($('#bVRHzLnkmSTJ').length == 0){
	displayAdBlockerMessage();
}
  </script>
  <script>
   var filter={
    staff:"",
    team:0,
    season:0,
    min_players:0,
    min_draftpicks:0,
    event:0,
    date_min:"0",
    date_max:"0",
    future_considerations:0,
    multiple_team:0,
    retained_salary:0,
    signing_rights:0,
    conditional_draft_pick:0,
    page:1,
    search:"",
    round_1:0,
    round_2:0,
    round_3:0,
    round_4:0,
    round_5:0,
    round_6:0,
    round_7:0,
};
var offset_keys={
    "date":"2021-04-10",
    "trade_id":6430}
function sticky(){
	if($(window).width()>600){
		$('#trade').stickyTableHeaders({fixedOffset:$('#nav')});
	}else{
		$('#trade').stickyTableHeaders('destroy');
	}
};
$(window).on('resize',function(){
	sticky();
});
sticky();
$(document).on('click','.navx_us',function(e){
	$(".navx").removeClass('navx_s').addClass('navx_us');;
	$(this).removeClass('navx_us').addClass('navx_s');
	if(this.id=="cha_n"){
		tab=1;
		$('.cha,.ahl').show();
		$('.ch,.sal').hide();
	}else if(this.id=="ch_n"){
		tab=2;
		$('.ch').show();
		$('.cha,.sal,.ahl').hide();
	}else if(this.id=="sal_n"){
		tab=3;
		$('.sal,.ahl').show();
		$('.ch,.cha').hide();
	}
});
$('body').on('change','#filter_team',function(e){
    e.preventDefault();
    filter.page = 1;
    filter.staff = "";
    url(true);
});

$('body').on('click','.season',function(e){
    e.preventDefault();
    filter.season = $(this).attr('data-season');
    filter.page = 1;
    $('.season').removeClass("hid_anc");
    $('.season[data-season='+filter.season+']').addClass("hid_anc");
    url(true);
});
$('body').on('change','#players_count',function(e){
    e.preventDefault();
    filter.min_players=$(this).val();
    filter.page = 1;
    url(true);
});
$('body').on('change','#dp_count',function(e){
    e.preventDefault();
    filter.min_draftpicks=$(this).val();
    filter.page = 1;
    url(true);
});
$('body').on('change','#event',function(e){
    e.preventDefault();
    filter.event=$(this).val();
    filter.page = 1;
    url(true);
});
$('body').on('change','#future_considerations',function(e){
    e.preventDefault();
    if(document.getElementById('future_considerations').checked) {
        filter.future_considerations=1;
    } else {
        filter.future_considerations=0;
    }
    filter.page = 1;
    url(true);
});
$('body').on('change','#multi_team',function(e){
    e.preventDefault();
    if(document.getElementById('multi_team').checked) {
        filter.multiple_team=1;
    } else {
        filter.multiple_team=0;
    }
    filter.page = 1;
    url(true);
});
$('body').on('change','#conditional_draft_pick',function(e){
    e.preventDefault();
    if(document.getElementById('conditional_draft_pick').checked) {
        filter.conditional_draft_pick=1;
    } else {
        filter.conditional_draft_pick=0;
    }
    filter.page = 1;
    url(true);
});
$('body').on('change','#signing_rights',function(e){
    e.preventDefault();
    if(document.getElementById('signing_rights').checked) {
        filter.signing_rights=1;
    } else {
        filter.signing_rights=0;
    }
    filter.page = 1;
    url(true);
});
$('body').on('change','#retained_salary',function(e){
    e.preventDefault();
    if(document.getElementById('retained_salary').checked) {
        filter.retained_salary=1;
    } else {
        filter.retained_salary=0;
    }
    filter.page = 1;
    url(true);
});
$('body').on('change','.round',function(e){
    e.preventDefault();
    var round=$(this).attr("data-round")
    if($(this).prop('checked')){
        filter["round_"+round] = 1;
    } else {
        filter["round_"+round] = 0;
    }
    filter.page = 1;
    url(true);
});
var y=new Date().getFullYear();
$('#from').datepicker({
	changeMonth:true,
	changeYear:true,
	yearRange:'2000:'+y,
	maxDate:'0',
	onClose:function(selectedDate) {
        $( "#to" ).datepicker("option","minDate",selectedDate);
        filter.date_min=selectedDate.replace(/\//g,'');
        if(filter.date_min!=0 && filter.date_max!=0)
            filter.page = 1;
      	    url(true);
    }
});
$( "#to" ).datepicker({
	changeMonth: true,
	changeYear:true,
	yearRange:'2000:'+y,
	maxDate:'0',
	onClose: function( selectedDate ) {
		$( "#from" ).datepicker("option","maxDate",selectedDate);
		filter.date_max=selectedDate.replace(/\//g,'');
		if(filter.date_min!=0 && filter.date_max!=0)
            filter.page = 1;
			url(true);
	}
});
var searchDelay = (function(){
    var timer = 0;
    return function (callback, ms){
        clearTimeout(timer);
        timer = setTimeout(callback, ms);
    };
})();
$('body').on('keyup','#player_search',function(){
    var search = $(this).val();
    searchDelay(function(){
        if(search.length>2){
            filter.search = search.replace(/[^0-9a-zA-Z_ -]/gi,'');
        }else{
            filter.search = "";
        }
        filter.page = 1;
        url(true);
    },300);
});
function getFilterTeam(){
    filter.team=$('#filter_team').val();
}
$('body').on('click','#load_button',function(e){
    e.preventDefault();
    $('#load_button').hide();
    $('#load_icon').show();
    url(false);
});
var url_var = "?";
function urlVar(){
    if(url_var == "?") {
        url_var = "&"; 
        return "?";
    }
    return "&";
}
function url(clear){
    getFilterTeam();
    var uri = "";
    url_var = "?";
    if(filter.staff!=""){
        uri += '/staff/'+filter.staff;
    }else{
        if(filter.team!=0){
            uri += '/'+filter.team;
        }
        if(filter.season!=0){
            uri += urlVar()+'season='+filter.season;
        }
        if(filter.min_players!=0){
            uri += urlVar()+'min-players='+filter.min_players;
        }
        if(filter.min_draftpicks!=0){
            uri += urlVar()+'min-draftpicks='+filter.min_draftpicks;
        }
        if(filter.event!=0){
            uri += urlVar()+'event=';
            if(filter.event==1) uri += "deadline";
            else if(filter.event==3) uri += "expansion";
            else uri += "draft";
        }
        if(filter.date_min!=0&&filter.date_max!=0){
            uri += urlVar()+'date='+filter.date_min+'-'+filter.date_max;
        }
        if(filter.future_considerations!=0){
            uri += urlVar()+'future-considerations';
        }
        if(filter.multiple_team!=0){
            uri += urlVar()+'multi-team';
        }
        if(filter.retained_salary!=0){
            uri += urlVar()+'retained-salary';
        }
        if(filter.conditional_draft_pick!=0){
            uri += urlVar()+'conditional-draft-pick';
        }
        if(filter.signing_rights!=0){
            uri += urlVar()+'signing-rights';
        }
        if(filter.search!=""){
            uri += urlVar()+'s='+encodeURIComponent(filter.search).replace(/%20/g, "+");
        }
        if(filter.round_1==1||filter.round_2==1||filter.round_3==1||filter.round_4==1||filter.round_5==1||filter.round_6==1||filter.round_7==1){
            var round_set = 0;
            uri += urlVar()+'round=';
            for(i=1;i<=7;i++){
                if(filter["round_"+String(i)] == 1){ 
                    if(round_set==1){
                        uri += '-';
                    }
                    uri += String(i);
                    round_set=1;
                }
            }
        }
    }
    History.replaceState('','Trades - CapFriendly','/trades'+uri);
    

    if (clear) {
        $('#cont_x').css('opacity',0.3);
        offset_keys.date="";
        offset_keys.trade_id=0;
    }
    uri += urlVar() + 'ajax';
    uri += urlVar() + 'offset-date='+offset_keys.date;
    uri += urlVar() + 'offset-trade_id='+offset_keys.trade_id;
    $.getJSON('/trades'+encodeURI(uri),function(d){
        if (clear) {
            $('#cont_x').css('opacity',1).empty();
        }
        $('#cont_x').append(d.data.html);
        if (d.meta.code == 200) {
            offset_keys.date=d.data.offset_keys.date;
            offset_keys.trade_id=d.data.offset_keys.trade_id;
        }
        if (d.data.result_count < 24 || d.meta.code != 200 || d.data.end == 1){
            $('#load').hide();
        }else{
            $('#load_button,#load').show();
            $('#load_icon').hide();
        }
    });
};
  </script>
 </body>
</html>